{
    "scripts": {
        "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
        "replica_start": "dfx start --background",
        "replica_stop": "dfx stop",
        "canister_deploy_local": "dfx deploy hotel_booking_ICP",
        "canister_deploy_mainnet": "dfx deploy --network ic hotel_booking_ICP",
        "canister_uninstall": "dfx canister uninstall-code hotel_booking_ICP",
        "canister_call_get_message": "dfx canister call hotel_booking_ICP getMessage",
        "canister_call_set_message": "dfx canister call hotel_booking_ICP setMessage '(\"Hello world!\")'"
    },
    "dependencies": {
        "azle": "0.19.0",
        "uuidv4": "^6.2.13"
    }
}
